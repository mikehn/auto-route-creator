  },
  "homepage": "https://github.com/mikehn/autoRouteJs#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.7.7",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.13",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
