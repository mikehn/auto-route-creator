  "bugs": {
    "url": "https://github.com/mikehn/autoRouteJs/issues"
  },
  "homepage": "https://github.com/mikehn/autoRouteJs#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.7.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "faker": "^5.5.3"
  }
}
