{
  "name": "auto-route-creator",
  "version": "2.0.4",
  "description": "An easy way to define maintain and verify routes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
