{
  "name": "auto-route-creator",
  "version": "1.2.6",
  "description": "An easy way to define maintain and verify routes",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "test": "babel src -d lib && node lib/test/SampleRoute.js",
    "build:npm": "SET NODE_ENV=production && if exist dist rmdir /s /q dist && mkdir dist && npx babel src --out-dir dist --copy-files",
